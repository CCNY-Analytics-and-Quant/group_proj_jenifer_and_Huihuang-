    "#slope = rise/run\n",
    "trend = df.rolling(52*5).mean()[-20:]\n",
    "\n",
    "df_factors[\"slope_52\"] = (trend.iloc[-1] - trend.iloc[0])/(52*5)\n",
    "df_factors"
