   "cell_type": "code",
   "execution_count": 275,
   "id": "0d83ea04-cf28-44ca-b30e-b957778cf020",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Metrics():\n",
    "    def __init__(self, data, tickers):\n",
    "        self.data = data\n",
    "        self.tickers = tickers\n",
    "        self.returns = self.data[self.tickers].pct_change().dropna()\n",
    "        self.weights = np.random.random(len(self.tickers))\n",
    "        self.weights /= np.sum(self.weights)\n",
    "      \n",
    "    def COV(self):\n",
    "        return self.returns.cov()\n",
    "    \n",
    "    def Weights(self):\n",
    "        weights = np.random.random(len(self.tickers))\n",
    "        weights /= np.sum(weights)\n",
    "        return weights\n",
    "  \n",
    "    \n",
    "    def portfolio_return(self,  weights):\n",
    "        return_p = (self.returns.mean()*252)@weights \n",
    "        return return_p\n",
    "    \n",
    "    def portfolio_var(self, weights, cov):\n",
    "        port_var = weights@(cov*252)@weights \n",
    "        return port_var\n",
    "    def sharpe_ratio(self, port_r, port_var):\n",
    "        rf = 0.02 #risk-free rate\n",
    "        sharpe = (return_p-rf)/np.sqrt(port_var)\n",
    "        return sharpe"
   ]
  },
