   "id": "32874640-78c1-45b3-9fb0-7990fcde1afd",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Solving AX = B\n",
    "def permutation_solve(P, b):\n",
    "    Pt = P.T\n",
    "    return pt@b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "15b8eec4-5dba-4727-8fd7-338bf51c796c",
   "metadata": {},
   "outputs": [],
   "source": [
    "#step 2 lower triangle\n",
    "def lower_triangle_solver(L,z):\n",
    "    n = len(z)\n",
    "    y = np.zeros(n)\n",
    "    y[0]= z[0]\n",
    "    for j in range(1, n):\n",
    "        y[j] = z[j] - L[j,:j].dot(y[:y])\n",
    "        return y"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "ecfbf62b-0d01-4079-9fd1-d39997512cae",
   "metadata": {},
   "outputs": [],
   "source": [
    "def upper_trian_solver(U, y):\n",
    "    n= len(y)\n",
    "    x = np.zeros(n)\n",
    "    x[n-1]=y[n-1]/U[n-1, n-1]\n",
    "    for i in range(n-2, -1, -1):\n",
    "        x[i] = (y[i]- U[i, i+1:].dot(x[i+1]))/U[i,i]\n",
    "        \n",
    "    return x"
   ]
  },
  {
