    "class Finance:\n",
    "    \n",
    "    def __init__(self, tickers, benchmarks, start, end, dtype):\n",
    "        self.tickers = tickers\n",
    "        self.benchmarks = benchmarks\n",
    "        self.start = start\n",
    "        self.end = end\n",
    "        self.dtype = dtype\n",
    "        \n",
    "    def data_all(self):\n",
    "        df = yf.download(self.tickers + self.benchmarks, start=self.start, end=self.end)[self.dtype]\n",
    "        df['portfolio'] = df[self.tickers].mean(axis = 1)\n",
    "        return df\n",
    "    #def data_tickers(self, dtype=\"Adj Close\"):\n",
    "        #return  yf.download(self.tickers, start=self.start, end=self.end)[dtype]\n",
    "    \n",
    "   # def data_benchmarks(self, dtype = \"Adj Close\"):\n",
    "        #return yf.download(self.benchmarks, start=self.start, end=self.end)[dtype]\n",
    "    \n",
    "    def weight(self):\n",
    "        return 1/len(self.tickers)\n",
    "    \n",
    "    def returns(self, data=None, period = 1, Type = 'daily', days=2520):\n",
    "        \n",
    "        if Type == 'daily':\n",
    "            \n",
    "            return data.pct_change(period)\n",
    "        \n",
    "        if Type == 'total':\n",
    "            \n",
    "            return data.pct_change(len(data) - 1)[-1:].T\n",
    "        \n",
    "        else:\n",
    "            \n",
    "            return data**(1/np.sqrt(days)) #because 10 years 252*10 = 2520\n",
    "            \n",
    "        \n",
    "            \n",
    "    def annual_volatility(self,returns = None, trailing = None, time=None):\n",
    "        \n",
    "        \"\"\"\n",
    "        Converting volatility (standard deviation) from annual to daily is pretty simple. \n",
    "        The thumb rule for calculation is that the volatility is proportional to the square \n",
    "        root of time, and not to time itself.\n",
    "        √252 * √Variance\n",
    "        \"\"\"\n",
    "        \n",
    "        if trailing:\n",
    "            \n",
    "            return (returns[:-trailing].std())*np.sqrt(time)\n",
    "                    #/returns[:-trailing].std())/np.sqrt(time)\n",
    "        else:\n",
    "                    \n",
    "            return (returns.std())*np.sqrt(time)\n",
    "        \n",
    "        \n",
    "    def beta(self,returns, trailing):\n",
    "        \n",
    "        \"\"\"\n",
    "        beta = cov(r_s, r_b)/std(r_s)\n",
    "        example:\n",
    "        beta = beta(returns, 252)\n",
    "        for bench in benchmarks:\n",
    "            relative_beta = beta[bench]\n",
    "            ---- produces individual values ----\n",
    "        ******\n",
    "        returns beta values in dataframe columns benchmarsk, rows = tickers\n",
    "        \n",
    "        \"\"\"\n",
    "        return returns[-trailing:].cov()/returns[-trailing:].var()\n",
    "    \n",
    "    def drawdown(self,returns= None, Type = 'max', window= 5):\n",
    "        \n",
    "        \"\"\"\n",
    "        ## Max Drawdown — \n",
    "        The worst possible return one could see, if they had bought high and sold low.\n",
    "        Maximum drawdown measures the maximum fall in the value of the investment, as given \n",
    "        by the difference between the value of the lowest trough and that of the highest peak before the trough.\n",
    "        \n",
    "        DD= (Trough Value−Peak Value)/Peak Value \t\n",
    "        max = how much it loses per sliding window\n",
    "        \n",
    "        \"\"\"\n",
    "        dd = returns.rolling(window).max() - returns.rolling(window).min()\n",
    "        if Type == 'max':\n",
    "            return dd.max()\n",
    "        else:\n",
    "            return dd.mean()\n",
    "        \n",
    "    "
   ]
  },
